+/* Determine locale's codepage for the WideCharToMultiByte.  */
+static unsigned int
+get_locale_code_page (void)
+{
+  UINT codepage = CP_ACP; /* ANSI codepage by default */
+  const char *current_locale = setlocale (LC_CTYPE, NULL);
+  const char *pdot = strrchr (current_locale, '.');
+
+  /* After a dot may follow a codepage number, or the "utf8" string.  */
+  if (pdot)
+    {
+      if (('u' == pdot[1] || 'U' == pdot[1]) &&
+        ('t' == pdot[2] || 'T' == pdot[2]) &&
+        ('f' == pdot[3] || 'F' == pdot[3]) &&
+        (('8' == pdot[4] && '\0' == pdot[5]) ||
+         ('-' == pdot[4] && '8' == pdot[5] && '\0' == pdot[6])))
+        codepage = CP_UTF8; /* 65001 */
+      else
+        {
+          /* Try to parse codepage number.  */
+          unsigned cp;
+          if (1 == sscanf (pdot + 1, "%u", &cp))
+            codepage = cp;
+        }
+    }
+
+  return codepage;
